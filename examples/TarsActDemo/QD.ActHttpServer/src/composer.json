    "require": {
        "phptars/tars-server": "~0.3",
        "phptars/tars-client": "~0.3",
        "phptars/tars-deploy": "~0.1",
        "phptars/tars2php": "~0.3",
        "phptars/tars-log": "~0.1",
        "ext-zip" : ">=0.0.1",
        "google/protobuf": "^3.8"
